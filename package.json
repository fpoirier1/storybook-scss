{
  "name": "c24-dashboard",
  "version": "1.0.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "clear && webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint src || true",
    "test": "clear && jest ./tests --setupTestFrameworkScriptFile=raf/polyfill",
    "dev": "webpack-dashboard -t 'Webpack Dashboard' -- webpack-dev-server",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "watch": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors --watch",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.11",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.10",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.11.0"
  },
  "dependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-hot-loader": "^3.1.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
